
<!--component/search/popups/index.wxml-->
<view class="wx-popup" hidden="{{flag}}">
  <view class='popup-container'>
    <view class="wx-popup-con">{{content}}</view>
    <view class="wx-popup-btn">
      <text class="btn-no" bindtap='error'>{{btn_no}}</text>
      <text class="btn-ok" bindtap='success'>{{btn_ok}}</text>
    </view>
  </view>
</view>