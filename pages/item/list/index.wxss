.page__bd {
  padding-bottom: 40rpx;
}

.weui-cells {
  position: relative;
  margin: 0;
  z-index: 1;
}

.weui-cells-no {
  position: relative;
  margin: 0;
  padding: 200rpx 0;
  z-index: 1;
  display: flex;
  justify-content: center;
}

.weui-cells:before {
  top: 0;
  border-top: 1rpx solid #d9d9d9;
}

.weui-cells:after {
  bottom: 0;
  border-bottom: 1rpx solid #d9d9d9;
}

.weui-cells-container {
  display: flex;
  flex-flow: column;
}


